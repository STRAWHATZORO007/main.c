#include <stdio.h>
int main()
{
    char ch;
    printf("enter the character\n");
    scanf("%c", &ch);
    if (ch == 'a' || ch == 'A' ||
        ch == 'e' || ch == 'E' ||
        ch == 'i' || ch == 'I' ||
        ch == 'o' || ch == 'O' ||
        ch == 'u' || ch == 'U')
    {
        printf("It is Vowel");
    }
    else
    {
        printf(" It is not vowel");
    }
    return 0;
}
